{
  "name": "club-website-v2",
  "homepage": "https://hcp-uw.github.io/club-website-v2",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "update-types": "npx supabase gen types --lang=typescript --project-id \"$PROJECT_REF\" > src/database.types.ts",
    "dev": "vite",
    "build": "CI=false && tsc -b && vite build",
    "build-for-vergil": "cross-env BUILD_FOR_VERGIL=true && tsc -b && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.10.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@octokit/core": "^6.1.4",
    "@supabase/supabase-js": "^2.46.0",
    "@tailwindcss/vite": "^4.0.9",
    "bootstrap": "^5.3.3",
    "framer-motion": "^11.18.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-github-calendar": "^4.1.6",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.25.1",
    "react-slick": "^0.30.3",
    "react-social-media-embed": "^2.5.18",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^4.0.9"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "supabase": "^2.22.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vite-plugin-singlefile": "^2.0.2"
  }
}